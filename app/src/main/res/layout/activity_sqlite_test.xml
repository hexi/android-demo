<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/save_no_transaction_then_get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_no_transaction_then_get"
        android:text="save_no_transaction_then_get" />

    <Button
        android:id="@+id/save_with_transaction_then_get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_with_transaction_then_get"
        android:text="save_with_transaction_then_get" />

    <Button
        android:id="@+id/get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="get"
        android:text="get" />

    <Button
        android:id="@+id/get_get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="get_get"
        android:text="get_get" />

    <Button
        android:id="@+id/save_with_transaction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_with_transaction"
        android:text="save_with_transaction" />

    <Button
        android:id="@+id/get_no_transaction_then__save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="get_no_transaction_then__save"
        android:text="get_no_transaction_then__save" />

    <Button
        android:id="@+id/get_then_save_with_transaction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="get_then_save_with_transaction"
        android:text="get_then_save_with_transaction" />

    <Button
        android:id="@+id/save_then_save_with_transaction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_then_save_with_transaction"
        android:text="save_then_save_with_transaction" />

    <Button
        android:id="@+id/save_with_transaction_then_save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_with_transaction_then_save"
        android:text="save_with_transaction_then_save" />

    <Button
        android:id="@+id/count_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="count_user"
        android:text="count_user" />

    <Button
        android:id="@+id/save_no_transaction_then_count_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_no_transaction_then_count_user"
        android:text="save_no_transaction_then_count_user" />

    <Button
        android:id="@+id/save_transaction_then_count_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save_transaction_then_count_user"
        android:text="save_transaction_then_count_user" />
</LinearLayout>