<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitchView">
        <attr name="textLeft" format="string" />
        <attr name="textRight" format="string" />
        <attr name="leftChecked" format="boolean" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CheckView">
        <attr name="checked" format="boolean" />
        <attr name="checkedImage" format="reference" />
        <attr name="uncheckedImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="NumberSettingView">
        <attr name="hint" format="string" />
        <attr name="enabled" format="boolean" />
        <attr name="inputType">
            <enum name="number" value="1" />
            <enum name="decimal" value="2" />
        </attr>
    </declare-styleable>
</resources>
